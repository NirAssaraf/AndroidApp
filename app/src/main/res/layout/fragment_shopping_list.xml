<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ShoppingListFragment">


    <TextView
        android:id="@+id/shoppingList_ProductName"
        android:layout_width="336dp"
        android:layout_height="38dp"
        android:layout_marginTop="124dp"
        android:text="Product Name"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/shoppingList_ProductPrice"
        android:layout_width="336dp"
        android:layout_height="38dp"
        android:text="Product Price"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/shoppingList_ProductQuantity"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/shoppingList_ProductQuantity"
        android:layout_width="336dp"
        android:layout_height="38dp"
        android:textSize="20dp"
        android:text="Quantity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/shoppingList_ProductName" />

    <CheckBox
        android:id="@+id/associated_Or_NotAssociated_checkBox"
        android:layout_width="30dp"
        android:layout_height="0dp"


        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/shoppingList_ProductName"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="126dp" />


</androidx.constraintlayout.widget.ConstraintLayout>